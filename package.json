    "server": "npm run server:dev",
    "server:dev": "webpack-dashboard -- webpack-dev-server --config config/webpack.serve.dev.js --inline  --watch --progress --profile --colors --display-error-details --display-cached",
    "server:prod": "http-server build/prod --cors"
  },
